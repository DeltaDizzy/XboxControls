<Window x:Class="XboxControls.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XboxControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle x:Name="rctPlayer" HorizontalAlignment="Left" Height="32" Margin="100,370,0,0" Stroke="Black" VerticalAlignment="Top" Width="32" Fill="#FFEA3939"/>
        <Label x:Name="lbltext" Content="" HorizontalAlignment="Left" Margin="559,166,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Rectangle x:Name="rctProjectile" HorizontalAlignment="Left" Height="12" Margin="110,380,0,0" Stroke="Black" VerticalAlignment="Top" Width="12" RenderTransformOrigin="0.5,0.5" Fill="#FF756565">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
    </Grid>
</Window>
